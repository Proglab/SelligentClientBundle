{
    "name": "shelf-utilities/selligent-client-bundle",
    "description": "Selligent Client Bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Fabrice",
            "email": "info@proglab.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ShelfUtilities\\SelligentClientBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-soap": "*",
        "symfony/framework-bundle": "^6.1",
        "symfony/http-kernel": "^6.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "phpstan/phpstan": "^0.12.88",
        "phpunit/phpunit": "^9.5",
        "symfony/test-pack": "^1.0",
        "symfony/phpunit-bridge": "^5.3"
    },
    "scripts": {
        "cs:dry": "vendor/bin/php-cs-fixer fix --diff --dry-run --no-interaction --ansi",
        "cs:fix": "vendor/bin/php-cs-fixer fix --ansi",
        "phpstan": "vendor/bin/phpstan analyse --ansi"
    },
    "license": "proprietary",
    "minimum-stability": "dev"
}
